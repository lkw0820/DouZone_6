package kosa.oop7ex3;

public class RefExam03 {
	
	interface A{
		int abc(String str);
	}

	public static void main(String[] args) {
		// 1
		A a1 = new A() {
			
			@Override
			public int abc(String str) {
				// TODO Auto-generated method stub
				
				return str.length();
			}
		};
		System.out.println(a1.abc("안녕"));
		
	}

}
