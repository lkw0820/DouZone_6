package kosa.video;

public class GeneralMember {
	String id;
	String name;
	String address;
	Video vo = new Video();
	
	
	public GeneralMember() {
		super();
	}


	public GeneralMember(String id, String name, String address, Video vo) {
		super();
		this.id = id;
		this.name = name;
		this.address = address;
		this.vo = vo;
	}


	public void info() {
		System.out.println(this.id);
		System.out.println(this.name);
		System.out.println(this.address);
		System.out.println(this.name);
		System.out.println(this.vo.no);
		System.out.println(this.vo.name);
		System.out.println(this.vo.actor);
	}
}
