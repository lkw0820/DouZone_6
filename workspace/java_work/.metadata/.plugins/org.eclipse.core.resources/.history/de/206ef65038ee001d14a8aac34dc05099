package kosa.oop;

public class AccountMain3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		CheckingAccount ca = new CheckingAccount();
		
		ca.setAccountNo("111-11");
		ca.setOwnerName("홍길동");
		ca.setBalance(10000);
		ca.setCardNo("111-11");
		
		try {
			ca.pay("111-11", 500);
		}catch(Exception e) {
			e.printStackTrace();
		}
		
		ca.print();
	}

}
