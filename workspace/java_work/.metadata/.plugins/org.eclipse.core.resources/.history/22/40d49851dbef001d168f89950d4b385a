package kosa.baseball;

public class Question {
	private int[] arr = new int[3];
			
	//생성자
	public Question() {
		arr[0]=(int)(Math.random())*9+1;
		for(int i=1;i<arr.length;i++) {
			while(arr[i]==arr[i-1]) {
				arr[i]=(int)(Math.random()*9)
						+1;
			}
		}
		
	}
	
	//get set
	public int[] getArr() {
		return arr;
	}
	public void setArr(int[] arr) {
		this.arr = arr;
	}
}


