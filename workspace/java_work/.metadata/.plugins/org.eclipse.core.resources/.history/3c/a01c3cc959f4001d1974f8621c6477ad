package kosa.stream;

public class Student {
	
		private String name;
		private int score;
		//생성자
		public Student() {
			super();
		}
		public Student(String name, int score) {
			super();
			this.name = name;
			this.score = score;
		}
		
		
		//set get
		public String getName() {
			return name;
		}
		public void setName(String name) {
			this.name = name;
		}
		public int getScore() {
			return score;
		}
		public void setScore(int score) {
			this.score = score;
		}
		
		

	
}
