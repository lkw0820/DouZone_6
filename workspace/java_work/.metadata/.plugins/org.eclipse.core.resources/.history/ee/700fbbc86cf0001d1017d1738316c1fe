package kosa.api;

public class Person {
	
	private String name;
	private int age;
	//생성자
	public Person() {
		super();
	}
	public Person(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}
	//method
	//@Override
	public boolean equals(Person p) {
		if(this.name.equals(p.getName())&&this.age==p.getAge()) {
			return true;
		}else {
			return false;
		}
		
	}
	
	
	//get set
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}

}
