package kosa.video;

import java.util.*;

public class Student {
	String name;
	List<Course> list = new ArrayList<Course>();
	
	public Student() {
		super();
	}
	
	public Student(String name) {
		super();
		this.name = name;
	}

	public void enrolment(Course course) {
		list.add(course);
	}
	
	public void show() {
		System.out.println("학생이름: "+this.name);
		for(Course c: list) {
			c.show();
		}
		System.out.println();
	}
}
