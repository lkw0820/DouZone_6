package kosa.basic;

public class ExceptionExam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=10;
		int b=0;
		try {
			int r=a/b;
		}catch(Exception e){
			//에러 정보 출력 방법
			e.printStackTrace();
			System.out.println(e.getMessage());
		}
		
		System.out.println("done");

	}

}
