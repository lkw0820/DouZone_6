package kosa.data;

public class ListExam {

	public static void main(String[] args) {
		// 1~45중복되지 않는 로또번호를 6개 출력
		//1.배열
		int[] arr1= new int[6];
		for(int i=0;i<arr1.length;i++) {
			arr1[i]=(int)(Math.random()*45)+1;
			for(int j=1;j<=i;j++) {
				if(arr1[i]==arr1[j]) {
					i--;
				}
			}
			System.out.println(arr1[i]);
		}
		
		//2.List(contains(정수))
		
		//3.Set(TreeSet()) =>중복 허용X
		

	}

}
