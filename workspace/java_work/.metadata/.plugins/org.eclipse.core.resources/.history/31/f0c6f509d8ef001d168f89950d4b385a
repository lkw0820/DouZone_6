package kosa.baseball;

public class Question {
	private int[] arr = new int[3];
			
	//생성자
	public Question() {
		for(int i=0;i<arr.length;i++) {
			arr[i]=(int)(Math.random())*9+1;
		}
	}
	
	//get set
	public int[] getArr() {
		return arr;
	}
	public void setArr(int[] arr) {
		this.arr = arr;
	}
}


