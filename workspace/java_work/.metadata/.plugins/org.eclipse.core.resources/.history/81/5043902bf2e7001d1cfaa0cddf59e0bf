package kosa.mission;

import java.util.Scanner;

public class Mission6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int cnt=0;
		int n = sc.nextInt();
		int[] arr = new int[n];
		for(int i =0;i<n-1;i++) {
			arr[i]=sc.nextInt();
			if(arr[i]>=arr[i+1]) {
				cnt++;
			}
		}
		System.out.println(n-cnt);
		

	}

}
