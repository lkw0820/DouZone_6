package kosa.oop3;

import java.awt.Button;
import java.awt.Frame;

public class MyFrame extends Frame{

	public MyFrame() {
		Button b  = new Button("Button");
		add(b);
		
		setSize(300,200);
		setLocation(300,200);
		setVisible(true);
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new MyFrame();
	}

}
