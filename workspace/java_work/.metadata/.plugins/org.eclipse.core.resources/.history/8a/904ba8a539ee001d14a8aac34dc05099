package kosa.video;

public class SpecialMember extends GeneralMember {
	private int bonus;

	//생성자
	public SpecialMember() {
		super();
	}

	public SpecialMember(int bonus) {
		super();
		this.bonus = bonus;
	}
	
	//method
	public void show() { 
		super.show3();
		System.out.println("회원의 보너스 포인터 적립"+ bonus);
	}

	//getter setter
	public int getBonus() {
		return bonus;
	}

	public void setBonus(int bonus) {
		this.bonus = bonus;
	}

}
