package kosa.oop7ex4;

interface A{
	B abc();
}
class B{
	B(){
		System.out.println("생성자");
	}
}

public class RefExam04 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//1
		A a1 = new A() {
			
			@Override
			public B abc() {
				// TODO Auto-generated method stub
				B b = new B();
				return b;
			}
		};
		//2 
		A a2 = () -> new B();
		//3
		A a3 = B :: new;

	}

}
