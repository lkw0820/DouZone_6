package kosa.api;

public class StringExam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//1.String 클래스 생성 방법의 차이점
		String str = "ABC";
		String str2 = "ABC";
		String str3 = new String("ABC");
		
		//2. String 클래스는 불변성 특징
		str.concat("DEF");
		System.out.println(str);
		
	}

}
